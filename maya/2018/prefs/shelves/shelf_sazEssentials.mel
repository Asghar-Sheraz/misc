        -annotation "// objSplineExport.mel\n//\n// 7/14/03\n// Immersion Medical Inc.\n// Mark Hovis\n//\n// Export current selection of degree 3 curves to .obj format\n// GUI provides text field input for output file name and comments.\n// Output is to an obj directory in the current project.\n// Directory is checked for existing files with the same name and prompts user if true.\n// Information about the source and creation of the .obj file provided at the head.\n//\n// usage: In Maya command line enter>  objSplineExport\n\n//////////////////////////////////ROUNDING PROCEDURE/////////////////////////\nglobal proc float roundoff( float $f, int $n )\n{\n// we divide if n < 0 to avoid numeric\n// precision problems\nif( $n > 0 )\n{\nfloat $roundScale = pow(10,$n);\nif( $f > 0 )\nreturn( ((float)(int)($f * $roundScale + 0.5)) /$roundScale );\nelse\nreturn( ((float)(int)($f * $roundScale - 0.5)) /$roundScale );\n}\nelse\n{\nfloat $roundScale = pow(10,-$n);\nif( $f > 0 )\nreturn( ((float)(int)($f/$roundScale + 0.5)) *$roundScale );\nelse\nreturn( ((float)(int)($f/$roundScale - 0.5)) *$roundScale );\n}\n}\n//////////////////////////////////////END ROUNDING PROCEDURE/////////////////\n\n// create window with input text fields\nglobal proc objSplineExport ()\n{\n\tif (`window -exists objSplineExport`) showWindow;\n\tstring $exportSpline = `window -title \"objSplineExport\" \n\t\t\t\t-in \"splineExport\" -w 408 -h 140 objSplineExportWindow`;\n\tcolumnLayout -adj true;\n\t\tseparator -style \"none\" -h 12;\n\t\tstring $splineFile = `textFieldGrp -label \"Output Obj File Name\"`;\n\t\ttext -label \"(.obj will be appended)       \" -align right;\n\t\tseparator -style \"none\" -h 12;\n//\t\tseparator -style \"out\" -h 12;\n//\t\tseparator -style \"none\" -h 12;\n//\t\tstring $splineComment = `textFieldGrp -label \"File Notes/Description\"`;\n\t\tseparator -style \"none\" -h 12;\n\t\t\n\tstring $exportCommand = (\"exportSplines\" + \" \" + $splineFile);\n\n\tframeLayout -marginWidth 0 -marginHeight 0 -bs etchedOut -labelVisible false;\n        rowColumnLayout -numberOfRows 1 -co 1 both 2;\n\t\tbutton -label \"Export Obj\"\n                \t-width 190\n                \t-command $exportCommand;\n\t\tbutton -label \"Cancel\"\n\t\t\t-width 190\n\t\t\t-command \"deleteUI objSplineExportWindow\";\n\tshowWindow objSplineExportWindow;\t\n\n} //******End of objSplineExport \n\nglobal proc exportSplines (string $splineFile)\n{\n\n\t//set output file\n\tstring $baseName = `textFieldGrp -q -text $splineFile`;\n\tstring $objName = ($baseName + \".obj\");\n\tstring $rootPath = `workspace -q -rd`;\n\tstring $objDir = `workspace -q -fre \"OBJ\"`;\n\tstring $pathFile = ($rootPath + $objDir + \"\\/\" + $objName);\n\t\n\t//check for existing .obj\n\t//int $checkExistObj = `system(\"test -f \" + $pathFile + \" && echo 0 || echo 1\")`;\n\tint $checkExistObj = fopen ($pathFile, \"r\");\n\tif ($checkExistObj > 0)\n\t{\n               string $overWrite = `confirmDialog -title \"Confirm\" -parent objSplineExportWindow \n\t\t\t\t\t-message \"File exists. Overwrite?\"\n               \t\t\t\t-button \"Yes\" -button \"No\" -defaultButton \"Yes\"\n               \t\t\t\t-cancelButton \"No\" -dismissString \"No\"`;\n                 if ($overWrite != \"Yes\"){\n\t\t\t//system(\"rm -f \" + $pathFile); \n                        return;\n\t\t\t}\n    }\n\tfclose ($checkExistObj);\n\t//header\n\tint $exportFile = fopen ($pathFile, \"w\");\n//\tfprint ($exportFile, \"# \" + $pathFile + \"\\n\");\n\tstring $currentScene = `file -q -sn`;\n//\tfprint ($exportFile, \"# Source: \" + $currentScene + \"\\n\");\n\tstring $exportTime = `system \"date\"`;\n//\tfprint ($exportFile, \"# Created: \" + $exportTime + \"\\n\");\n//\tstring $splnComment = `textFieldGrp -q -text $splineComment`;\n//\tfprint ($exportFile, \"# Notes/Description: \" + $splnComment + \"\\n\");\n//\tfprint ($exportFile, \"\\n\");\n\n\n\n\t//export stuff\n\tstring $currentList[] = `ls -sl`;\n\tstring $currentSel;\n\tint $cvCount = 0;\n\n\t//write cv's\n\tfor ($currentSel in $currentList)\n\t{\n\t\tstring $cvNumbers = \" \";\n\t\tfprint ($exportFile, \"g \" + \"\\n\");\n//\t\tfprint ($exportFile, \"g Obj_\" + $currentSel + \"\\n\");\n\t\tint $cvs = `getAttr -size ($currentSel + \".cp\")`;\n\t\tfor ($i=0; $i<$cvs; $i++) \n\t\t{\n\t\t\tfloat $cvPos[] = `getAttr ($currentSel + \".cp[\" + $i + \"]\")`;\n\t\t\tfloat $cvX = eval(\"roundoff \" + $cvPos[0] + \" 6\");\n\t\t\tfloat $cvY = eval(\"roundoff \" + $cvPos[1] + \" 6\");\n\t\t\tfloat $cvZ = eval(\"roundoff \" + $cvPos[2] + \" 6\");\n\t\t\tfprint ($exportFile, \"v \" + $cvX + \" \" + $cvY + \" \" +$cvZ + \"\\n\");\n\t\t\t$cvCount += 1;\n\t\t\t$cvNumbers = ($cvNumbers + $cvCount + \" \"); \n\t\t}\n\t\n\t\t//write curve info\n\t\tfprint ($exportFile, \"cstype bspline\" + \"\\n\");\n\t\tfprint ($exportFile, \"deg 3\" + \"\\n\");\n\t\tfprint ($exportFile, \"g Obj_\" + $currentSel + \"\\n\");\n\t\tint $min = `getAttr ($currentSel + \".min\")`;\n\t\tint $max = `getAttr ($currentSel + \".max\")`;\n\t\tfprint ($exportFile, \"curv \" + $min + \" \" + $max + $cvNumbers + \"\\n\");\n\n\t\t//write knot info\n\t\tstring $splineInfo = `createNode curveInfo`;\n\t\tstring $splineOut = ($currentSel + \".worldSpace\");\n\t\tstring $infoInput = ($splineInfo + \".inputCurve\");\n\t\tconnectAttr $splineOut $infoInput;\n\t\tfloat $knotList[] = `getAttr ($splineInfo + \".knots\")`;\n\t\tint $knotSize = `getAttr -size ($splineInfo + \".knots\")`;\n\t\tfprint ($exportFile, \"parm u \" + $knotList[0] + \" \");\n\t\tint $knotNum;\n\t\tfor ($knotNum in $knotList)\n\t\t{\n\t\t\tfprint ($exportFile, $knotNum + \" \");\n\t\t}\n\t\tfprint ($exportFile, $knotNum + \"\\n\");\n//\t\tfprint ($exportFile, \"end\" + \"\\n\" + \"\\n\");\n\t\tfprint ($exportFile, \"end\" + \"\\n\");\n\t\tdelete $splineInfo;\n\t}\n\tfprint ($exportFile, \"g\");\n\tfclose $exportFile;\n\tint $sysCheckExportSize = `system(\"test -s \" + $pathFile + \" && echo 0 || echo 1\")`;\n\tif ($sysCheckExportSize == 0)\n\t\t{\n\t\tprint (\"Result: \" + $pathFile + \"\\n\");\n\t\tdeleteUI -window objSplineExportWindow;\n\t\t}\n\t   else\n\t\tprint (\"Result: file not written \\n\");\n\tselect -r $currentList;\n}" 
